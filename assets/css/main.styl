.bg-none
	background none
$normail-color = #ca4341
.bg-normail
	background $normail-color
	color #FFFFFF
#app
	color #333
.checkBox
	width 10px
	height 10px
	border-radius 50%
	border 1px solid #9E9E9E
.checked
	background $normail-color
.containerBox{
	width 100%	
	height calc(100vh-44px)
}
.s-checkBox
	width 40upx
	height 40upx
	display inline-block
	background gray
	font-size 40upx
	line-height 40upx
	color #ffffff
.check-green
	background green
.s-lineH1{
	line-height 1
}
.s-pointerEvents{
	pointer-events: none;
}
.s-miniWidth{
	width 80upx
}
.s-navbar{
	.flex-sub{
		height 90upx
	}
}
.s-none{
	display none
}

.fixed {
		position: fixed;
		z-index: 99;
	}
	.VerticalBox,.box{
		height: 100%;
	}
	.VerticalNav.nav {
		width: 200upx;
		white-space: initial;
	}

	.VerticalNav.nav .cu-item {
		width: 100%;
		text-align: center;
		background-color: #fff;
		margin: 0;
		border: none;
		height: 50px;
		position: relative;
	}

	.VerticalNav.nav .cu-item.cur {
		background-color: #f1f1f1;
	}

	.VerticalNav.nav .cu-item.cur::after {
		content: "";
		width: 8upx;
		height: 30upx;
		border-radius: 10upx 0 0 10upx;
		position: absolute;
		background-color: currentColor;
		top: 0;
		right: 0upx;
		bottom: 0;
		margin: auto;
	}

	.VerticalBox {
		display: flex;
	}

	.VerticalMain {
		background-color: #f1f1f1;
		flex: 1;
	}